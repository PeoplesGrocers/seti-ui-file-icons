{
  "transform": {
    "^.+\\.(t|j)s?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/seti-ui/",
    "<rootDir>/node_modules/"
  ]
}